{
    "experiment": {
        "timesteps": 200000,
        "num_runs": 10,
        "env": "LunarLander-v2",
        "log_name": "evo_ac_ll_test_large_lr",
        "log_run": false,
        "print_interval": 1,
        "log_interval": 2000,
        "log_path": "checkpoints/ll",
        "test_strat": "weightedvote"
    },

    
    "evo_ac": {
        "pop_size": 10,
        "value_coeff": 0.5,
        "entropy_coeff": 0.05,
        "hold_elite":true,
        "recomb_nums": [5,3,1],
        "learning_rate": [1e-5, 1e-5],
        "mut_scale":  0.0,
        "lr_decay": 0.98,
        "gamma": 0.99

    },

    "neural_net": {
        "learning_rate":1e-3,
        "shared": [
            {
                "type": "Linear",
                "params": [8, 128], 
                "kwargs": {"bias":true}
            }, 
            {
                "type": "ReLU",
                "params": [], 
                "kwargs": {}
            }

        ],
        "policy":
        [
            {
                "type": "Linear",
                "params": [128, 4], 
                "kwargs": {"bias":true}
            }
        ],
        "value": [
            {
                "type": "Linear",
                "params": [128, 1], 
                "kwargs": {"bias":true}
            }
        ]
        
    }
}